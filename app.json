{
  "expo": {
    "name": "StuPlan",
    "slug": "last",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon-pm.png",
    "scheme": "last",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        // "foregroundImage": "./assets/images/icon-pm.png",
        // "backgroundImage": "./assets/images/icon-pm.png",
        "monochromeImage": "./assets/images/icon-pm.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.last"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-pm.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon-pm.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          // "sounds": [
          //   "./local/assets/notification_sound.wav",
          //   "./local/assets/notification_sound_other.wav"
          // ],
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
