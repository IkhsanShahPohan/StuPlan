{
  "expo": {
    "name": "StuPlan",
    "slug": "last",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon-pm.png",
    "scheme": "last",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "monochromeImage": "./assets/images/icon-pm.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.last"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#fff",
          "image": "./assets/splash-icon.png",

          "dark": {
            "image": "./assets/splash-icon-dark.png",
            "backgroundColor": "#fff"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon-pm.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "74b9a731-6366-49da-8ae5-b8253903c5f4"
      }
    }
  }
}
